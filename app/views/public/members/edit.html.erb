  <h1>会員情報編集</h1>

    <%= form_with model:@member,url: members_my_page_path, method: :patch do |f| %>
      <table>
        <%= render "public/members/member-info", f: f %>
      </table>
      <%= f.submit "編集内容を保存", class: "btn btn-success" %>
      <%= link_to "退会する",  members_unsubscribe_path, class: "btn btn-danger" %>
    <% end %>



